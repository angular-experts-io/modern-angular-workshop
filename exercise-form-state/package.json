{
  "name": "exercise-form-state",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all --parallel serve serve:api",
    "serve": "ng serve -o",
    "serve:api": "json-server --watch db.json --delay 750 --port 4300 --routes db-routes.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --config projects/product-app/jest.config.js",
    "test:watch": "npm run test -- --watch",
    "lint": "ng lint",
    "ci": "npm run lint && npm run test && npm run build",
    "analyze": "ng build --stats-json --output-hashing none --named-chunks && esbuild-visualizer --template treemap --metadata dist/product-app/stats.json --filename dist/product-app/analyse/index.html && http-server -o -c-1 ./dist/product-app/analyse/",
    "analyze:sme": "ng build --source-map --output-hashing none --named-chunks && source-map-explorer dist/product-app/browser/*.js --html dist/product-app/sme/index.html && http-server -o -c-1 ./dist/product-app/sme/",
    "format:write": "prettier \"projects/**/*.{ts,scss,json,html,js}\" --write",
    "format:test": "prettier  \"projects/**/*.{ts,scss,json,html,js}\" --list-different"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.1",
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^19.0.1",
    "@angular/compiler": "^19.0.1",
    "@angular/core": "^19.0.1",
    "@angular/forms": "^19.0.1",
    "@angular/material": "^19.0.0",
    "@angular/platform-browser": "^19.0.1",
    "@angular/platform-browser-dynamic": "^19.0.1",
    "@angular/router": "^19.0.1",
    "chart.js": "^4.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^19.0.2",
    "@angular/cli": "^19.0.2",
    "@angular/compiler-cli": "^19.0.1",
    "@types/jest": "^29.5.12",
    "angular-eslint": "18.4.2",
    "autoprefixer": "^10.4.20",
    "esbuild-visualizer": "^0.6.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-boundaries": "^5.0.1",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.4.1",
    "nwsapi": "^2.2.13",
    "json-server": "^0.17.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "8.15.0"
  }
}
